{
  "AllowedHosts": "*",

  // Logging
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Warning"
    }
  },

  // Authentication
  "Url": "http://YourIP:5000",
  "User": "",
  "Password": "",

  // Misc
  "Delay": 5000,

  // AI
  "AI": {
    "Type": "DeepStack",
    "Url": "http://YourIP:83",
    "MinSizeX": 10,
    "MinSizeY": 10
  },

  // Notifications
  "Notifier": {
    "Type": ""
  },

  // Cameras
  "Cameras": [
    {
      "Name": "Example1",
      "Types": [ "Person" ],
      "Threshold": 50
    },
    {
      "Name": "Example2",
      "Types": [ "Car", "Truck" ],
      "Threshold": 75
    }
  ]
}
